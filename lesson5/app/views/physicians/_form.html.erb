<%= form_with model: @physician do |form| %>
  <section>
    Имя<br>
    <%= form.text_field(:first_name, {:class => 'form-control'} ) %>
  </section>
  <section>
    Фамилия<br>
    <%= form.text_field(:last_name, {:class => 'form-control'} ) %>
  </section>
  <section>
    Уровень<br>
    <%= form.search_field(:level, {:class => 'form-control'} ) %>
  </section>
  <section>
      Телефон<br>
      <%= form.text_field(:phone, {:class => 'form-control'} ) %>
  </section>
  <%= form.submit({:class => 'btn btn-success mt-2'} ) %>
<% end %>
